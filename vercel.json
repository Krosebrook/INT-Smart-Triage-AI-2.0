{
  "version": 2,
  "name": "int-smart-triage-ai-2.0",
  "builds": [
    {
      "src": "api/vercel/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/triage",
      "dest": "/api/vercel/triage.js"
    },
    {
      "src": "/data/(.*)",
      "dest": "/public/data/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "api/vercel/triage.js": {
      "maxDuration": 10
    }
  }
}