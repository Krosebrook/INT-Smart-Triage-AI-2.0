{
  "name": "int-smart-triage-ai-2.0",
  "version": "1.0.0",
  "description": "Secure, production-ready AI Triage Tool for INT Inc. Client Success",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:types": "tsc --project tsconfig.json",
    "preview": "vite preview",
    "deploy": "vercel --prod",
    "lint": "eslint . --ext .js,.cjs,.ts,.tsx --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.cjs,.ts,.tsx --ignore-path .gitignore --fix",
    "format": "prettier --write \"**/*.{js,cjs,ts,tsx,json,md,html,css}\"",
    "format:check": "prettier --check \"**/*.{js,cjs,ts,tsx,json,md,html,css}\"",
    "test": "npm run build:types && node --test test/*.test.js",
    "test:coverage": "npm run build:types && c8 --reporter=text --reporter=html --reporter=lcov node --test test/*.test.js",
    "test:coverage-check": "npm run build:types && c8 --check-coverage --lines 70 --functions 70 --branches 70 node --test test/*.test.js",
    "validate": "npm run format:check && npm run lint && npm run build:types && npm test && npm run build"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^7.0.0",
    "@dnd-kit/sortable": "^9.0.0",
    "@supabase/supabase-js": "^2.39.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "undici": "^6.3.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.2",
    "@vercel/node": "^3.2.15",
    "@vitejs/plugin-react": "^4.3.4",
    "c8": "^10.1.3",
    "eslint": "^8.55.0",
    "prettier": "^3.6.2",
    "typescript": "^5.6.3",
    "vercel": "^32.4.1",
    "vite": "^5.0.0"
  },
  "keywords": [
    "triage",
    "ai",
    "customer-support",
    "vercel",
    "supabase"
  ],
  "author": "INT Inc.",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "c8": {
    "include": [
      "src/**/*.{js,ts,tsx}",
      "api/**/*.{js,ts}"
    ],
    "exclude": [
      "test/**",
      "**/*.test.js",
      "node_modules/**",
      "dist/**"
    ],
    "all": true
  }
}
