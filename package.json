{
  "name": "int-smart-triage-ai-2.0",
  "version": "1.0.0",
  "description": "Secure, production-ready AI Triage Tool for INT Inc. Client Success",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "vercel --prod",
    "lint": "eslint . --ext .js,.cjs --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.cjs --ignore-path .gitignore --fix",
    "format": "prettier --write \"**/*.{js,cjs,json,md,html,css}\"",
    "format:check": "prettier --check \"**/*.{js,cjs,json,md,html,css}\"",
    "test": "node --test test",
    "test:coverage": "c8 --reporter=text --reporter=html --reporter=lcov node --test test",
    "test:coverage-check": "c8 --check-coverage --lines 70 --functions 70 --branches 70 node --test test",
    "validate": "npm run format:check && npm run lint && npm test && npm run build"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.2",
    "undici": "^6.3.0"
  },
  "devDependencies": {
    "c8": "^10.1.3",
    "eslint": "^8.55.0",
    "jsdom": "^27.0.1",
    "prettier": "^3.6.2",
    "vercel": "^32.4.1",
    "vite": "^5.0.0"
  },
  "keywords": [
    "triage",
    "ai",
    "customer-support",
    "vercel",
    "supabase"
  ],
  "author": "INT Inc.",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "c8": {
    "include": [
      "src/**/*.js",
      "api/**/*.js"
    ],
    "exclude": [
      "test/**",
      "**/*.test.js",
      "node_modules/**",
      "dist/**"
    ],
    "all": true
  }
}
